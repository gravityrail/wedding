<%= content_for :title do %>
<h2>Domain Summary</h2>
<% end %>

<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @domain.name %>
</p>

<p>
  <b>Uses hsts:</b>
  <%= @domain.uses_hsts %>
</p>

<p>
  <b>Uses https:</b>
  <%= @domain.uses_https %>
</p>

<p>
  <b>Mixed content:</b>
  <%= @domain.mixed_content %>
</p>

<p>
  <b>All https:</b>
  <%= @domain.all_https %>
</p>

<p>
  <b>All identifying:</b>
  <%= @domain.all_identifying %>
</p>

<p>
  <b>Secure cookies:</b>
  <%= @domain.secure_cookies %>
</p>

<p>
  <b>Ssl valid:</b>
  <%= @domain.ssl_valid %>
</p>

<p>
  <b>Key strength:</b>
  <%= @domain.key_strength %>
</p>

<p>
  <b>Notes:</b>
  <%= @domain.notes %>
</p>


<%= link_to raw('&laquo; Domain List'), domains_path %>
| <%= link_to 'Edit', edit_domain_path(@domain) %>