<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <% @attendees.each do |user| %>
  <Placemark>
    <name><%= user.name %></name>
    <description><%= user.address %></description>
    <Point>
      <coordinates><%= user.lat %>,<%= user.lon %>,0</coordinates>
    </Point>
  </Placemark>
  <% end %>
</kml>